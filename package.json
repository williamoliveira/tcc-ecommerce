{
  "name": "react-isomorphic-kit-boilerplate",
  "version": "1.0.0",
  "description": "React Isomorphic Kit Boilerplate",
  "license": "MIT",
  "main": "build/server/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "babel-node ./internal/scripts/build.js --optimize",
    "build:dev": "babel-node ./internal/scripts/build.js",
    "clean": "babel-node ./internal/scripts/clean.js",
    "develop": "babel-node ./internal/scripts/develop.js",
    "lint": "eslint src",
    "start": "cross-env NODE_ENV=production node build/server",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "precommit": "lint-staged",
    "format": "prettier-eslint \"+(src)/**/*.js\" --write"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|node_modules|flow-typed|public)/"
    ]
  },
